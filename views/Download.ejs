    <!-- Main Site -->
    <main id="site-main">
        <div class="container">

            <!-- form handling -->
            <form  id="download">
                <table class="table">
                    <thead class="thead-dark">
                        <tr>
                            <th>ID</th>
                            <th>Name</th>
                            <th>Link</th>
                            <th>action</th>
                        </tr>
                    </thead>
                    <tbody>
                        <%- include('include/_show_link') %>
                    </tbody>
                </table>
                <div class="form-group">
                    <span id="Downloading-notification"></span>
                </div>
                <div class="form-group">
                    <button type="button" onclick ="download()" class="btn text-dark update">Download all Files</button>
                </div>
            </form>

        </div>
    </main>
<!-- /Main Site -->


